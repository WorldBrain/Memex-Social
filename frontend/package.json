{
  "name": "frontend",
  "version": "0.3.1",
  "private": true,
  "scripts": {
    "start": "concurrently -n DS,FE -p name \"yarn dev-server\" \"yarn client\"",
    "client": "cross-env BROWSER=none webpack serve --config webpack/webpack.dev.js",
    "prepare": "true",
    "dev-server": "nodemon --watch ./dev-server --watch ./src/services/fixtures/utils.ts --exec 'ts-node --project dev-server/tsconfig.json' dev-server/src/main.ts",
    "generate-types": "ts-node --project scripts/tsconfig.json scripts/generate-types.ts",
    "generate-rules": "ts-node --project scripts/tsconfig.json scripts/generate-rules.ts ../firebase",
    "karma": "karma",
    "firebase": "cd ../firebase && firebase",
    "firebase:build-fns": "cd ../firebase/functions; yarn build; cd -",
    "build:deptree": "depcruise --exclude '^node_modules' --output-type dot ./src | dot -T svg > /tmp/dependencygraph.svg",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --max-warnings=0",
    "test:base": "TS_NODE_PROJECT=tsconfig.mocha.json mocha --require ts-node/register --require ignore.loader.js",
    "test": "yarn test:base 'src/**/*.test.ts'",
    "test:watch:base": "yarn test:base --watch --watch-extensions ts",
    "test:watch": "yarn test:watch:base 'src/**/*.test.ts'",
    "firebase-emulator:setup": "yarn firebase setup:emulators:firestore && yarn firebase setup:emulators:database && yarn firebase setup:emulators:storage",
    "firebase-emulator:start": "yarn firebase -P worldbrain-staging emulators:start --only functions,database,firestore,auth,storage",
    "create:storage-module": "cp -r code-templates/storage-module.ts src/storage/modules/${MODULE}.ts",
    "create:ui-component": "cp -r code-templates/ui-element src/main-ui/components/${ELEMENT}",
    "create:ui-container": "cp -r code-templates/ui-element src/main-ui/containers/${ELEMENT}",
    "create:ui-page": "cp -r code-templates/ui-element src/main-ui/pages/${ELEMENT}",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "build:base": "REACT_APP_BACKEND=firebase env-cmd -f $ENV_FILE webpack --config webpack/webpack.prod.js && mkdir -p ../firebase/public && rm -rf ../firebase/public/* && mv build/* ../firebase/public/ && cp -r public/* ../firebase/public/",
    "build:staging": "ENV_FILE=.env.staging yarn build:base",
    "build:production": "ENV_FILE=.env.production.local yarn build:base",
    "deploy:base": "yarn firebase -P $FIREBASE_PROJECT deploy --only hosting",
    "deploy:staging": "yarn build:staging && FIREBASE_PROJECT=worldbrain-staging yarn deploy:base",
    "deploy:production": "yarn build:production && FIREBASE_PROJECT=worldbrain-1057 yarn deploy:base"
  },
  "proxy": "http://localhost:5030/",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
