{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently -n DS,FE -p name \"yarn dev-server\" \"yarn client\"",
    "profile": "webpack --profile --json --config webpack.config.babel.js > ./dist/profile.json && webpack-bundle-analyzer ./dist/profile.json",
    "client": "webpack --env mode=dev --env isDevServer --env NODE_ENV=local serve --config webpack.config.babel.js",
    "prepare": "true",
    "dev-server": "nodemon --watch ./dev-server --exec 'ts-node --project dev-server/tsconfig.json' dev-server/src/main.ts",
    "generate-types": "ts-node --project scripts/tsconfig.json scripts/generate-types.ts",
    "karma": "karma",
    "firebase": "cd ../firebase && firebase",
    "build": "node scripts/build.js",
    "build:deptree": "depcruise --exclude '^node_modules' --output-type dot ./src | dot -T svg > /tmp/dependencygraph.svg",
    "test": "TS_NODE_PROJECT=tsconfig.mocha.json mocha --require ts-node/register --require ignore.loader.js 'src/**/*.test.ts'",
    "test:watch": "TS_NODE_PROJECT=tsconfig.mocha.json mocha --watch --watch-extensions ts --require ts-node/register  --require ignore.loader.js 'src/**/*.test.ts'",
    "create:storage-module": "cp -r code-templates/storage-module.ts src/storage/modules/${MODULE}.ts",
    "create:ui-component": "cp -r code-templates/ui-element src/main-ui/components/${ELEMENT}",
    "create:ui-container": "cp -r code-templates/ui-element src/main-ui/containers/${ELEMENT}",
    "create:ui-page": "cp -r code-templates/ui-element src/main-ui/pages/${ELEMENT}",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "build:unfinished": "webpack --config webpack.config.babel.js",
    "build:base": "REACT_APP_BACKEND=firebase env-cmd -f $ENV_FILE node scripts/build.js && rm -rf ../firebase/public/* && mv build/* ../firebase/public/",
    "build:staging": "ENV_FILE=.env.staging yarn build:base",
    "build:production": "ENV_FILE=.env.production.local yarn build:base",
    "deploy:base": "yarn firebase -P $FIREBASE_PROJECT deploy --only hosting",
    "deploy:staging": "yarn build:staging && FIREBASE_PROJECT=worldbrain-staging yarn deploy:base",
    "deploy:production": "yarn build:production && FIREBASE_PROJECT=worldbrain-1057 yarn deploy:base"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "modulePaths": [],
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "dependencies": {
    "@josephg/resolvable": "1.0.0",
